{
  "name": "peach-x",
  "version": "1.0.9",
  "description": "A Simple, Fast Vue 3 Component Library built with TypeScript",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./resolver": {
      "import": "./dist/resolver/index.mjs",
      "require": "./dist/resolver/index.cjs"
    },
    "./components": {
      "import": "./dist/components/index.mjs",
      "require": "./dist/components/index.cjs"
    },
    "./components/*": {
      "import": "./dist/components/*/index.mjs",
      "require": "./dist/components/*/index.cjs"
    },
    "./theme/style.css": {
      "default": "./dist/theme/style.css"
    },
    "./theme/dark.css": {
      "default": "./dist/theme/dark.css"
    },
    "./theme": {
      "import": "./dist/theme/index.mjs",
      "require": "./dist/theme/index.cjs"
    }
  },
  "typesVersions": {
    "*": {
      "components/*": [
        "dist/types/components/*/index.d.ts"
      ],
      "resolver": [
        "dist/types/resolver/index.d.ts"
      ],
      "theme": [
        "dist/types/theme/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "build": "vite build",
    "gen:dts": "node --loader ts-node/esm scripts/generate-global-dts.ts"
  },
  "keywords": [
    "vue3",
    "component",
    "ui",
    "peach-x"
  ],
  "author": "zhouxk",
  "license": "MIT",
  "packageManager": "pnpm@10.11.0",
  "devDependencies": {
    "@types/node": "^22.15.18",
    "@vitejs/plugin-vue": "^5.2.4",
    "eslint": "^9.27.0",
    "rollup-plugin-copy": "^3.5.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^4.5.4",
    "unplugin-auto-import": "^19.2.0",
    "unplugin-vue-components": "^28.5.0"
  },
  "peerDependencies": {
    "vue": "^3.3.0"
  }
}
